{
    "configurations": [
        {
            "name": "ESP-IDF",
            "compilerPath": "${env:HOME}/.espressif/tools/xtensa-esp-elf/esp-13.2.0_20230928/xtensa-esp-elf/bin/xtensa-esp32-elf-gcc",
            "compileCommands": "${workspaceFolder}/build/compile_commands.json",
            "cStandard": "c11",
            "cppStandard": "c++20",
            "intelliSenseMode": "gcc-x64",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/components/**",
                "${workspaceFolder}/main",
                "${workspaceFolder}/build/config",
                "${env:HOME}/esp@5.2/v5.2.2/esp-idf/components/**",
                "${env:HOME}/.espressif/tools/xtensa-esp-elf/esp-13.2.0_20230928/xtensa-esp-elf/xtensa-esp32-elf/include/**"
            ],
            "defines": [
                "ESP_PLATFORM",
                "IDF_VER=\"v5.2.2\"",
                "CONFIG_IDF_TARGET_ESP32=1"
            ],
            "browse": {
                "path": [
                    "${workspaceFolder}",
                    "${env:HOME}/esp@5.2/v5.2.2/esp-idf/components"
                ],
                "limitSymbolsToIncludedHeaders": false
            }
        }
    ],
    "version": 4
}
